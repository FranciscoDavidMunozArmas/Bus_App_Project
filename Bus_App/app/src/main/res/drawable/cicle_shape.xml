<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
            <stroke android:color="@color/blue_normal"
                android:width="1dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <padding
                android:bottom="2dp"
                android:left="2dp"
                android:right="2dp"
                android:top="2dp" />
            <solid android:color="#10CCCCCC" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <padding
                android:bottom="2dp"
                android:left="2dp"
                android:right="2dp"
                android:top="2dp" />
            <solid android:color="#20CCCCCC" />
            <size android:width="200dp"
                android:height="200dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <padding
                android:bottom="2dp"
                android:left="2dp"
                android:right="2dp"
                android:top="2dp" />
            <solid android:color="#30CCCCCC" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <padding
                android:bottom="2dp"
                android:left="2dp"
                android:right="2dp"
                android:top="2dp" />
            <solid android:color="#50CCCCCC" />
        </shape>
    </item>

    <item>
        <shape android:shape="oval"
            android:useLevel="false">
            <solid android:color="@color/white" />
            <padding android:left="10dp"
                android:right="10dp"
                android:top="10dp"
                android:bottom="10dp"/>
            <stroke android:color="@color/blue_normal"
                android:width="5dp"/>
        </shape>
    </item>
    <size android:width="200dp"
        android:height="200dp"/>
</layer-list>